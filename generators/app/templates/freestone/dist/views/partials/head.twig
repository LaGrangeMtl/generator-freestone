
<meta charset="utf-8">
<meta name="viewport" content="width=device-width,initial-scale=1"/>
<meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
<link rel="stylesheet" href="/assets/css/index.{{ version }}.css">

{% if noindex %}<meta name="robots" content="noindex">{% endif %}

{% for language in languages %}
	{% if language.key != lang %}
		<link rel="alternate" hreflang="{{ language.code }}" href="{{ fs_link({l: language.key}) }}" />
	{% endif %}
{% endfor %}
<link rel="alternate" hreflang="{{ lang }}" href="{{ fs_link({l: lang}) }}" />

<script>
	document.querySelector('html').classList.add('has-js');

	(function(ns){
		ns.lang = '{{ lang }}';
	})(window.<%= props.projectNamespace %> = window.<%= props.projectNamespace %> || {});
</script>